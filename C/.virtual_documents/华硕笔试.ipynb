#include <stdio.h>
typedef union
{
    int a;
    char b[2];
}uniA;
void main()
{
    uniA A;
    A.a = 10;
    A.b[0] = 0;
    A.b[1] = 1;

    uniA B;
    A.b[0] = 0;
    A.b[1] = 1;
    B.a = 10;
    printf("%d,%d\n", A.a|B.a, (A.b[1]^B.b[1])|(A.b[0]^B.b[0]));
}





