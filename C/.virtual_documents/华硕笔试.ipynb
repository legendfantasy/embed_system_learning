typedef struct LNode
{
    int data;
    struct LNode *next;
};
//参数 head 是一线性链表的头指针，若链表有结尾，结尾指针为NULL
bool a(LNode *head)
{
    LNode *slow = head, *fast = head;
    while (fast && slow)
    {
        slow = slow->next;
        if (fast->next == NULL)
            fast = fast->next;
        else
            fast = fast->next->next;
        if (slow == fast)
            break;
    }
    return !(fast == NULL);
}
bool b(LNode *head)
{
    LNode *t_head = head;
    if(head == NULL)
        return false;
    LNode *p1 = head;
    LNode *p2 = head->next;
    LNode *p3 = NULL;
    t_head->next = NULL;
    while(p2 != NULL)
    {
        if(p2 == t_head)
            return true;
        p3 = p2->next;
        p2->next = p1;
        p1 = p2;
        p2 = p3;
    }
    return false;
}











